body {
  margin: 0;
  font-family: "Roboto", sans-serif;

  font-size: 14px;
  line-height: 1.6;
  color: #413d4b;
}

*,
*:after,
*:before {
  box-sizing: border-box;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
}

/*container*/
.container {
  width: 100%;
  max-width: 1200px;
  margin: 0 auto;
}

/*Intro*/
.intro {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 100%;
  height: 100vh;
  background: url(../img/images/intro.jpg) center no-repeat;
  background-size: cover;
  -webkit-background-size: cover;
}
/*контейнер для текста по середине*/
.intro__inner {
  width: 100%;
  max-width: 780px;
  margin: 0 auto;
  text-align: center;
}
/*текст*/
.intro__title {
  font-family: "Playfair Display", serif;
  font-size: 32px;
  font-weight: 900;
  color: #fff;
}
/*полоска*/
.intro__title:after {
  content: "";
  display: block;
  width: 50px;
  height: 1px;
  background-color: #00e0d0;
  margin: 30px auto 30px;
}

/*оформление текста под титулом*/
.intro__subtitle {
  font-size: 14px;
  font-weight: 300;
  color: #fff;
  margin: auto auto 50px;
}

/* header */
.header {
  padding-top: 15px;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1000;
}
/*выравнивание лого и навигации*/
.header__inner {
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}
/*выравнивание лого и текста*/
.logo {
  display: flex;
  flex-wrap: nowrap;
}
/*текст лого*/
.header__logo {
  font-size: 30px;
  font-weight: 100;
  color: #23899c;
  align-items: center;
}
/*иконка лого*/
.logo__icon {
  position: relative;
}
/*навигация*/
.nav {
  font-size: 13px;
  text-transform: uppercase;
}
/*оформление навигации*/
.nav__link {
  display: inline-block;
  vertical-align: top;
  margin: 0 15px;
  font-family: "Roboto";
  font-weight: 900;
  color: #fff;
  text-decoration: none;

  transition: color 0.3s linear;
}
/*цвет при наведении*/
.nav__link:hover {
  color: #00e0d0;
}

/*кнопки*/
.btn {
  display: inline-block;
  vertical-align: top;
  padding: 8px 12px;
  border: 1px solid #00e0d0;

  font-family: "Hammersmith One", sans-serif;
  font-weight: 400;
  color: #00e0d0;
  font-size: 16px;
  text-transform: uppercase;
  text-decoration: none;

  transition: color 0.3s linear;
}

.btn:hover {
  background-color: #00e0d0;
  color: #000;
}

/*section*/
.section {
  padding: 35px 0;
}

.container__section {
  width: 100%;
  max-width: 780px;
  display: flex;
  flex-wrap: nowrap;
  margin: 0 auto;
  justify-content: space-between;
}
.section__header {
  width: 100%;
  max-width: 500px;
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 30px;
  text-align: left;
}

.section__logo {
  width: 100%;
  max-width: 200px;
}
.section__text {
  font-weight: 300;
  font-size: 14px;
}
.section__title {
  font-family: "Playfair Display", serif;
  font-size: 22px;
  font-weight: 900;
}

.section__back {
  width: 100%;
  height: 100vh;
  background: url(../img/images/section_back.jpg) center no-repeat;
  background-size: cover;
  -webkit-background-size: cover;
  display: flex;
  justify-content: center;
  flex-direction: column;
}
.section__playicon {
  margin: 30px auto;
}
.section__textplayicon {
  color: #fff;
  font-size: 16px;
  font-weight: 900;
  text-transform: uppercase;
  text-align: center;
  /* margin: 0 auto; */
}

